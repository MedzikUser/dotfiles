{
    "workbench.colorCustomizations": {
        // Color of editor active line number
        "editorLineNumber.activeForeground": "#ffffff",
        // Color of editor line numbers
        "editorLineNumber.foreground": "#777777",

        // Foreground color of inline hints
        "editorInlayHint.foreground": "#868686f0",
        // Background color of inline hints
        "editorInlayHint.background": "#3d3d3d48",

        // rust-analyzer types color
        // Foreground color of inline hints for types
        "editorInlayHint.typeForeground": "#fdb6fdf0",
        // Foreground color of inline hints for parameters
        "editorInlayHint.parameterForeground": "#fdb6fdf0",

        // GitLens blame color
        // https://github.com/gitkraken/vscode-gitlens/issues/1089#issuecomment-675109373
        "gitlens.trailingLineForegroundColor": "#575757f0",

        // "[Material Theme Ocean]": {
